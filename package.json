{
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "apollo-server": "^2.15.1",
    "apollo-server-lambda": "^2.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "graphql": "^15.0.0",
    "graphql-middleware": "^6.0.3",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.34.1",
    "prisma": "^2.21.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "prettier": "^2.0.5",
    "serverless": "^2.8.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "^6.8.0",
    "serverless-offline-sqs": "^4.1.1",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^8.10.1",
    "typescript": "^4.1.0"
  },
  "license": "UNLICENSED",
  "name": "api-test-task",
  "scripts": {
    "dev": "env-cmd -f .env.development -- serverless offline start",
    "prisma:format": "prisma format",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:nexus": "TYPEGEN=1 env-cmd -f .env.development -- ts-node-transpile-only src/graphql/schema.ts",
    "generate:prisma": "prisma generate",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "db:migrate": "env-cmd -f .env.development -- prisma migrate dev --preview-feature",
    "db:seed": "env-cmd -f .env.development -- prisma db seed --preview-feature"
  },
  "version": "0.0.1"
}
